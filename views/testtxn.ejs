							<table border="1" class="ui celled striped table">
								<tbody>
									<tr>
										<th>S.No</th>
										<th>Label</th>
										<th>Value</th>
									</tr>
									<tr>
										<td>1</td>
										<td>
											<label>ORDER_ID::*</label>
										</td>
										<td>
											<div class="field">
											<input id="ORDER_ID" tabindex="1" maxlength="20" size="20" name="ORDER_ID" autocomplete="off" value='<%= orderId %>'
											 >
											</div>
										</td>
									</tr>
									<tr>
										<td>2</td>
										<td>
											<label>CUSTID ::*</label>
										</td>
										<td>
											<div class="field">
